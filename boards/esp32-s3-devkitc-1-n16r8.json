{
  "name": "ESP32-S3 DevKitC-1 N16R8 (OPI PSRAM)",
  "url": "https://www.espressif.com/en/products/devkits/esp32-s3-devkitc-1",
  "vendor": "Espressif",

  "build": {
    "arduino": {
      "ldscript": "esp32s3_out.ld",
      "memory_type": "qio_opi"
    },
    "core": "esp32",
    "extra_flags": [
      "-DBOARD_HAS_PSRAM"
    ],
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "qio",
    "mcu": "esp32s3",
    "variant": "esp32s3"
  },

  "connectivity": [
    "wifi",
    "bluetooth"
  ],

  "frameworks": [
    "arduino",
    "espidf"
  ],

  "upload": {
    "flash_size": "16MB",
    "maximum_ram_size": 327680,
    "maximum_size": 16777216,
    "require_upload_port": true,
    "speed": 921600
  }
}
